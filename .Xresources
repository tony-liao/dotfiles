! urxvt
! Appearance
URxvt.scrollBar: false
URxvt.font: xft:Hack Nerd Font:size=12
URxvt.boldFont: xft:Hack Nerd Font:size=12:style=Bold
URxvt.italicFont: xft:Hack Nerd Font:size=12:style=Italic
URxvt.internalBorder: 16
! Enable rgba (transparency)
URxvt.depth: 32
! Disable fake transparency
URxvt.transparent: false
URxvt.background: rgba:1d1d/2020/2121/e8e8

! Ctrl-Shift-C/V for X clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! Handle urls
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1

